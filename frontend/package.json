{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "cd .. && husky frontend/.husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,css,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@iconify/vue": "^4.3.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.8",
    "@vueuse/core": "^12.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint-plugin-vue": "^9.32.0",
    "lodash": "^4.17.21",
    "lucide-vue-next": "^0.477.0",
    "marked": "^15.0.7",
    "ofetch": "^1.4.1",
    "post-archiver": "^0.2.5",
    "radix-vue": "^1.9.17",
    "responsive-image-sizes": "^1.0.3",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "vanilla-lazyload": "^19.1.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "3.5.2",
    "reka-ui": "^2.0.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0",
    "vue-tsc": "^2.2.6"
  }
}
